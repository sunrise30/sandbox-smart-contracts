    "axios": "^0.18.0",
    "base32.js": "^0.1.0",
    "buidler-deploy": "^0.3.4",
    "buidler-ethers-v5": "^0.2.0",
    "buidler-gas-reporter": "^0.1.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "commander": "^3.0.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-mocha": "^0.0.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-mocha": "^6.3.0",
